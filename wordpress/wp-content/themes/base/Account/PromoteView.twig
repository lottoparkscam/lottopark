{#                {% else %}#}
                    <p class="promoteCompanyInfo">{{'Promote %s\'s services and earn! Simply share your reflink on social media, blog, or any other place and receive a comission for each referred player.' | trans | replace({'%s': whitelabelName})}}</p>
{#                {% endif %}#}
            </div>
            <div class="promoteLinkGroup">
                <div class="promoteLinkName">
                    {{'Your general reflink' | trans}}
                </div>
                <div class="promoteLink">
                    <input id="input-promote-link-lottery" type="text" value="{{ lotteryReferralUrl }}" class="form-control" readonly>
                    <button class="btn js-copy-to-clipboard" data-copy-target="input-promote-link-lottery"><i class="fa fa-copy"></i></button>
                </div>
            </div>
            <div class="promoteLinkGroup">
                <div class="promoteLinkName">
                    {{'Your casino reflink' | trans}}
                </div>
                <div class="promoteLink">
                    <input id="input-promote-link-casino" type="text" value="{{ casinoReferralUrl }}" class="form-control" readonly>
                    <button class="btn js-copy-to-clipboard" data-copy-target="input-promote-link-casino"><i class="fa fa-copy"></i></button>
                </div>
            </div>
        </div>
        <div class="promoteQrBox">
            <div class="promoteQrPanel">
                <div class="promoteQrCodeLottery">
                    <div class="promoteQrCodeTitle">{{'General QR Code' | trans}}</div>
                    <img src="{{ lotteryQrCodeInBase64 }}" alt="Your general QR code" />
                    <a href="{{ lotteryQrCodeInBase64 }}" download="qrCodeLottery" class="btn btn-primary">{{'Download QR code' | trans}}</a>
                </div>
                <div class="promoteQrCodeCasino">
                    <div class="promoteQrCodeTitle">{{'Casino QR Code' | trans}}</div>
                    <img src="{{ casinoQrCodeInBase64 }}" alt="Your general QR code" />
                    <a href="{{ casinoQrCodeInBase64 }}" download="qrCodeCasino" class="btn btn-primary">{{'Download QR code' | trans}}</a>
                </div>
            </div>
        </div>
    </div>
    <div class="promotePanelDetails">
        <div class="promoteDetailsRow">
            <div class="promoteDetailCol">
                <div class="promoteDetailNumber">{{ totalCommission|raw }}</div>
                <p>{{'Total earnings' | trans}}</p>
            </div>
            <div class="promoteDetailCol">
                <div class="promoteDetailNumber">{{ totalCommissionThisMonth|raw }}</div>
                <p>{{'Revenue this month' | trans}}</p>
            </div>
            <div class="promoteDetailCol">
                <div class="promoteDetailNumber">{{ registerCount }}</div>
                <p>{{'Registrations:' | trans | replace({':': ''})}}</p>
            </div>
            <div class="promoteDetailCol">
                <div class="promoteDetailNumber">{{ clickCount }}</div>
                <p>{{'Clicks' | trans}}</p>
            </div>
        </div>
    </div>
    <div class="promotePanelStatistics">
        <div class="promoteStatisticTables">
            <div class="promoteStatisticBox">
                <div class="promoteStatisticHeader">
                    <div class="promoteStatisticTitle">{{'Statistics' | trans}}</div>
                    <ul class="promoteStatisticNavigation">
                        <li><a href="#" class="btnChangeDataSet active" data-type="total">{{'Total' | trans}}</a></li>
                        <li><a href="#" class="btnChangeDataSet" data-type="last30Days">{{'30 days' | trans}}</a></li>
                        <li><a href="#" class="btnChangeDataSet" data-type="last60Days">{{'60 days' | trans}}</a></li>
                    </ul>
                </div>
                <div class="promoteStatisticBody">
                    <div class="promoteStatisticTable">
                        <div class="promoteStatisticTableRow">
                            <div class="promoteStatisticTableParam">{{'Earnings' | trans}}</div>
                            <div class="promoteStatisticTableValue" data-type="total">
                                {{ totalCommission|raw }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last30Days" style="display: none">
                                {{ totalCommissionLast30Days|raw }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last60Days" style="display: none">
                                {{ totalCommissionLast60Days|raw }}
                            </div>
                        </div>
                        <div class="promoteStatisticTableRow">
                            <div class="promoteStatisticTableParam">{{'Clicks' | trans}}</div>
                            <div class="promoteStatisticTableValue" data-type="total">
                                {{ clickCount }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last30Days" style="display: none">
                                {{ clickCountLast30Days }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last60Days" style="display: none">
                                {{ clickCountLast60Days }}
                            </div>
                        </div>
                        <div class="promoteStatisticTableRow">
                            <div class="promoteStatisticTableParam">{{'Registration' | trans}}</div>
                            <div class="promoteStatisticTableValue" data-type="total">
                                {{ registerCount }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last30Days" style="display: none">
                                {{ registerCountLast30Days }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last60Days" style="display: none">
                                {{ registerCountLast60Days }}
                            </div>
                        </div>
                        <div class="promoteStatisticTableRow">
                            <div class="promoteStatisticTableParam">{{'Active players' | trans}}</div>
                            <div class="promoteStatisticTableValue" data-type="total">
                                {{ activeUsersCount }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last30Days" style="display: none">
                                {{ activeUsersCountLast30Days }}
                            </div>
                            <div class="promoteStatisticTableValue" data-type="last60Days" style="display: none">
                                {{ activeUsersCountLast60Days }}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="promoteStatisticBox">
                <div class="promoteStatisticHeader withBorderBottom">
                    <div class="promoteStatisticTitle">{{'Your plans' | trans}}</div>
                </div>
                <div class="promoteStatisticBody">
                    <div class="promoteStatisticTable promotePlans">
                        <div class="promoteStatisticTableRow">
                            <div class="promoteStatisticTableParam">{{'Lottery 1st tier sale commission value:' | trans}}</div>
                            <div class="promoteStatisticTableValue">{{ groupData['firstTierLotterySaleCommission'] }}%</div>
                        </div>
{#                        <div class="promoteStatisticTableRow">#}
{#                            <div class="promoteStatisticTableParam">{{'Lottery 2nd tier sale commission value:' | trans}}</div>#}
{#                            <div class="promoteStatisticTableValue">{{ groupData['secondTierLotterySaleCommission'] }}%</div>#}
{#                        </div>#}
                    </div>
                </div>
            </div>
        </div>
        <div class="promoteStatisticChart">
            <div class="promoteStatisticBox">
                <div class="promoteStatisticHeader">
                    <div class="promoteStatisticTitle">{{'Earnings over the last 30 days' | trans}}</div>
                </div>
                <div class="promoteStatisticBody">
                    <div class="promoteStatisticMonth">
                        <div class="promoteDetailNumber"><span id="totalCommissionLast30Days">{{ totalCommissionLast30Days|raw }}</span></div>
                    </div>
                    <div id="promoteChart"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    window.promoteChartData = {{ promoteChartData|json_encode|raw }};
</script>
